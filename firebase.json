{
  "hosting": {
    "public": "dist/public",
    "rewrites": [
      {
        "source": "api/federation",
        "function": "backend-federation"
      }
    ],
    "headers": [
      {
        "source": ".well-known/stellar.toml",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }]
  },
  "functions": {
    "source": "frontend/functions"
  }
}
